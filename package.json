{
	"name": "draw-things-mcp-cursor",
	"version": "1.3.8",
	"description": "Draw Things API integration for Cursor using Model Context Protocol (MCP)",
	"private": false,
	"type": "module",
	"main": "dist/index.js",
	"bin": {
		"draw-things-mcp-cursor": "./dist/index.js"
	},
	"scripts": {
		"start": "node --experimental-vm-modules --no-warnings dist/index.js",
		"dev": "NODE_OPTIONS='--loader ts-node/esm' ts-node src/index.ts",
		"build": "rimraf dist && swc src -d dist --copy-files --strip-leading-paths",
		"test": "node --experimental-vm-modules --no-warnings test-mcp.js",
		"prepare": "npm run build",
		"prepublishOnly": "npm run build",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@modelcontextprotocol/sdk": "^1.6.0",
		"axios": "^1.8.0",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@swc/cli": "^0.3.8",
		"@swc/core": "^1.4.8",
		"@swc/helpers": "^0.5.6",
		"@types/node": "^20.12.8",
		"rimraf": "^5.0.10",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3"
	},
	"files": [
		"dist",
		"README.md",
		"cursor-setup.md"
	],
	"keywords": [
		"cursor",
		"mcp",
		"draw-things",
		"ai",
		"image-generation",
		"stable-diffusion"
	],
	"author": "James Jao",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/james-jao/draw-things-mcp"
	},
	"bugs": {
		"url": "https://github.com/james-jao/draw-things-mcp/issues"
	},
	"homepage": "https://github.com/james-jao/draw-things-mcp#readme",
	"engines": {
		"node": ">=16.0.0"
	}
}
